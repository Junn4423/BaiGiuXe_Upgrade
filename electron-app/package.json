{
  "name": "parking-lot-electron",
  "version": "1.0.0",
  "description": "Parking Lot Management System - Electron App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "NODE_ENV=development electron .",
    "build": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "ffmpeg-static": "^5.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^37.0.0",
    "electron-builder": "^26.0.12",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "build": {
    "appId": "com.parkinglot.app",
    "productName": "Parking Lot Management",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "rtsp-server.js",
      "../frontend/build/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    }
  }
}
